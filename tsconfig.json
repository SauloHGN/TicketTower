{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "paths": {
      // "@spartan-ng/ui-table-helm": [
      //   ".\\src/app/components/spartan/ui-table-helm/src/index.ts"
      // ],
      // "@spartan-ng/ui-button-helm": [
      //   ".\\src/app/components/spartan/ui-button-helm/src/index.ts"
      // ],
      // "@spartan-ng/ui-dialog-helm": [
      //   ".\\src/app/components/spartan/ui-dialog-helm/src/index.ts"
      // ],
      // "@spartan-ng/ui-helm-helm": [
      //   ".\\src/app/components/spartan/ui-helm-helm/src/index.ts"
      // ],
      // "@spartan-ng/ui-icon-helm": [
      //   ".\\src/app/components/spartan/ui-icon-helm/src/index.ts"
      // ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
